{
  "name": "${PROJECT_NAME}",
  "version": "1.0.0",
  "description": "${PROJECT_DESCRIPTION}",
  "main": "dist/index.js",
  "scripts": {
    "start": "set DEBUG=*,-babel && npm run build && node dist/index.js",
    "build": "babel src -d dist",
    "pub": "npm run build && npm publish --registry=http://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+${GITHUB}.git"
  },
  "keywords": [],
  "author": "${AUTHOR} <${EMAIL}>",
  "bugs": {
    "url": "${GITHUB}/issues"
  },
  "homepage": "${GITHUB}/#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5"
  }
}
